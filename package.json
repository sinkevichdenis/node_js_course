{
  "name": "node_js_course",
  "version": "0.0.1",
  "description": "NodeJS course",
  "scripts": {
    "start:1-1": "nodemon lesson_01/task_01",
    "start:1-2": "nodemon lesson_01/task_02 --exec babel-node",
    "transpile": "babel ./lesson_01 --out-dir dist",
    "server:2": "nodemon lesson_02/app.js",
    "client:2": "npm run start --prefix lesson_02/client",
    "lint": "eslint . --ext .ts",
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run server:2\" \"npm run client:2\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sinkevichdenis/node_js_course.git"
  },
  "author": "Dzianis Sinkevich1",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sinkevichdenis/node_js_course/issues"
  },
  "homepage": "https://github.com/sinkevichdenis/node_js_course#readme",
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "@types/express": "^4.17.7",
    "@types/nconf": "^0.10.0",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "concurrently": "^5.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "nodemon": "^2.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "csvtojson": "^2.0.10",
    "express": "^4.17.1",
    "nconf": "^0.10.0"
  }
}
